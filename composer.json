{
  "name": "greenn-labs/bubblewrap-sandbox",
  "description": "Security layer to run external commands inside bubblewrap for Laravel 5 through 12.",
  "keywords": ["laravel", "bubblewrap", "sandbox", "security", "bwrap"],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "illuminate/support": "5.*|6.*|7.*|8.*|9.*|10.*|11.*|12.*",
    "symfony/process": "^3.4|^4.4|^5.4|^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.6"
  },
  "autoload": {
    "psr-4": {
      "Greenn\\Libs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greenn\\Libs\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Greenn\\Libs\\Laravel\\BubblewrapServiceProvider"
      ],
      "aliases": {
        "BubblewrapSandbox": "Greenn\\Libs\\Laravel\\BubblewrapSandbox"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
