{
  "name": "greenn-labs/laravel-bwrap-guard",
  "description": "Camada de segurança para execução de comandos externos com Bubblewrap em projetos Laravel 5 a 12.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "illuminate/support": "12.*",
    "symfony/process": "^3.4|^4.4|^5.4|^6.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.6"
  },
  "autoload": {
    "psr-4": {
      "Greenn\\\\Sandbox\\\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greenn\\\\Sandbox\\\\Tests\\\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Greenn\\\\Sandbox\\\\Laravel\\\\BubblewrapServiceProvider"
      ],
      "aliases": {
        "Sandbox": "Greenn\\\\Sandbox\\\\Laravel\\\\Sandbox"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
